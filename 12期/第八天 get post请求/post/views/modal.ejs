<script>
    var isLogin = <%= isLogin %>;
    var regUser1 = <%= regUser1 %>;

    alert(isLogin);
    if (isLogin == true) {
        document.body.innerHTML ='恭喜您登录成功'
        setTimeout(function () {
            location.href='/user'
        },3000)

    } else if (isLogin == false) {
        if(!regUser1){
            document.body.innerHTML ='用户名错误！';
        }else{
            document.body.innerHTML ='密码错误！';
        }

        setTimeout(function () {
            location.href='/';
        },3000)
    }
</script>
